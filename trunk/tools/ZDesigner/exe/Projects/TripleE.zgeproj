  sin(CurrentModel.Personality+ this.V.X*0.3434)+cos(this.V.Y*5.223)+sin(this.V.Z*2.22);

Temp2=noise2(Temp1*0.8,Temp2 + CurrentModel.Personality*3.333) * 0.8;
