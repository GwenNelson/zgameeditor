/*Temp1=1-abs(this.V.X);
this.V.X*=1+Temp1;

Temp1=1-abs(this.V.Z);
this.V.Z*=1+Temp1;*/

Temp1=
  sin(CurrentModel.Personality+ this.V.X*2.2)+cos(this.V.Y*3.223)+sin(this.V.Z*2);
