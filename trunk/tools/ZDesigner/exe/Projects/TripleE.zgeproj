  if(abs(Temp3)>4.5) {
    MonkeyEmitNutTimer=0.05;//clamp(0.1 + MonkeyPersonality,0.1,0.2);
    MonkeyShotCount-=1;
    Temp1=1;
  }
}
//exit temp1=1 emit nut]]>
                      </Expression>
                    </ZExpression>
                    <Condition Expression="return Temp1==1;">
