Temp1=
  (noise2(this.X*3.4,this.Y*3.5) +
  noise2(this.X*6,this.Y*6.6)) * 1.8;
Temp1=abs(Temp1)*9.9;
this.Pixel.R=Temp1;
this.Pixel.G=Temp1*0.5;
this.Pixel.B=Temp1*0.5;
//this.Pixel.A=0.6;]]>
