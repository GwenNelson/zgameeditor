  391  
  392  Statement<var OutOp : TZcOp>
  393  = (. OutOp := nil; .)
  394  /* "const" Type ident "=" Expr<Op> {"," ident "=" Expr<Op>} ";" */
  395  /*IF IsLocalVarDecl THEN
  396  BEGIN
  397    LocalVarDecl ";"
  398  END
  399  |  EmbeddedStatement<OutOp> */
  400  
  401  LocalVarDecl<OutOp> ";"
  402  | EmbeddedStatement<OutOp>
  403    /* LL(1) confict between LocalVarDecl and StatementExpr: *
  404     * ident {"." ident} { "[" ...                           */
  405  .
  406  
  407  /*------------------------------------------------------------------------*/
  408  EmbeddedStatement<var OutOp : TZcOp>
  409    (. var
  410         SwitchValueOp,Op,IfOp,IfBody,ElseOp : TZcOp;
  411         ForInitOp,ForCondOp,ForIncOp,ForBodyOp : TZcOp;
  412         WhileCondOp,WhileBodyOp : TZcOp;
  413         SwitchOp : TZcOpSwitch;
  414         Loc : TZcOpLocalVar;
  415    .)
  416  = Block<OutOp>
  417  | ";"   (. OutOp := MakeOp(zcNop); .)
  418  | StatementExpr<OutOp> ";"
  419  | "if" (. ElseOp := nil; .)
  420    "(" Expr<IfOp> ")" EmbeddedStatement<IfBody> ["else" EmbeddedStatement<ElseOp>]
  421          (. OutOp := MakeOp(zcIf,[IfOp,IfBody,ElseOp]); .)
  422   | "switch" "(" Expr<SwitchValueOp> ")"
  423        (.
  424           Loc := TZcOpLocalVar.Create(nil);
  425           Loc.Id := MakeTempName;
  426           Loc.Typ := SwitchValueOp.GetDataType;
  427           SymTab.Add(Loc.Id,Loc);
  428           CurrentFunction.AddLocal(Loc);
  429  
  430           OutOp := MakeOp(zcBlock);
  431           OutOp.Children.Add( MakeAssign(atAssign, MakeOp(zcIdentifier,Loc.Id),SwitchValueOp) );
  432  
  433           SwitchOp := TZcOpSwitch.Create(nil);
  434           SwitchOp.ValueOp := MakeIdentifier(Loc.Id);
  435           OutOp.Children.Add(SwitchOp);
  436        .)
  437     "{" {
  438       (. SwitchOp.CaseOps.Add( MakeOp(zcNop) ); SwitchOp.StatementsOps.Add( MakeOp(zcNop) ); .)
  439       SwitchSection<SwitchOp>
  440     } "}"
  441   | "while"
  442       (. try
  443           SymTab.PushScope;
  444           WhileCondOp := nil; WhileBodyOp := nil; .)
  445     "(" Expr<WhileCondOp> ")" EmbeddedStatement<WhileBodyOp>
  446     (.
  447           OutOp := MakeOp(zcWhile,[WhileCondOp,WhileBodyOp]);
  448         finally
  449           SymTab.PopScope;
  450         end;
  451     .)
  452   /* | "do" EmbeddedStatement<Op> "while" "(" Expr<Op> ")" ";" */
  453   | "for"
  454      (. try
  455           SymTab.PushScope;
  456           ForInitOp :=nil; ForCondOp := nil; ForIncOp := nil; .)
  457     "(" [ForInit<ForInitOp>] ";" [Expr<ForCondOp>] ";" [ForInc<ForIncOp>] ")" EmbeddedStatement<ForBodyOp>
  458     (.
  459           OutOp := MakeOp(zcForLoop,[ForInitOp,ForCondOp,ForIncOp,ForBodyOp]);
  460         finally
  461           SymTab.PopScope;
  462         end;
  463     .)
  464  /* | "foreach" "(" Type ident "in" Expr<Op> ")" EmbeddedStatement<Op> */
  465   | "break" ";"    (. OutOp := MakeOp(zcBreak); .)
  466   | "continue" ";" (. OutOp := MakeOp(zcContinue); .)
  467   | (. Op := nil; .) "return" [ Expr<Op> ] ";"
  468      (.
  469          if (Op=nil) then
  470          begin
  471            if CurrentFunction.ReturnType.Kind<>zctVoid then
  472              SynError(202)
  473            else
  474              OutOp := MakeOp(zcReturn);
  475          end else
  476          begin
  477            if CurrentFunction.ReturnType.Kind=zctVoid then
  478              SynError(203)
  479            else
  480              OutOp := MakeOp(zcReturn,[ MakeCompatible(Op,CurrentFunction.ReturnType) ]);
  481          end;
  482      .)
  483  .
