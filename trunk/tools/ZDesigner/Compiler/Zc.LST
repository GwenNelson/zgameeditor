  539  /*------------------------------------------------------------------------*/
  540  
  541  ForInit<var OutOp : TZcOp>     (. var Op : TZcOp; .)
  542  = (. Op:=nil; OutOp := MakeOp(zcBlock); .)
  543  (
  544  LocalVarDecl<Op> (. if Assigned(Op) then OutOp.Children.Add(Op); .)
  545  | (. Op:=nil; .) StatementExpr<Op> (. if Assigned(Op) then OutOp.Children.Add(Op); .)
  546    { (. Op:=nil; .) "," StatementExpr<Op> (. if Assigned(Op) then OutOp.Children.Add(Op); .) }
  547  )
  548  .  
  549  
  550  /*------------------------------------------------------------------------*/
  551  
  552  ForInc<var OutOp : TZcOp>      (. var Op : TZcOp; .)
  553  = (. Op:=nil; 
  554       OutOp := MakeOp(zcBlock);
  555    .) 
  556  (
  557  StatementExpr<Op> (. if Assigned(Op) then OutOp.Children.Add(Op); .) 
  558  { (. Op:=nil; .) "," StatementExpr<Op> (. if Assigned(Op) then OutOp.Children.Add(Op); .) }
  559  )
  560  . 
