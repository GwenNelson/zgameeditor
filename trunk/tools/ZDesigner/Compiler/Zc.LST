  593  ForInit<var OutOp : TZcOp>     (. var Op : TZcOp; .)
  594  = (. Op:=nil; OutOp := MakeOp(zcBlock); .)
  595  (
  596  LocalVarDecl<Op> (. if Assigned(Op) then OutOp.Children.Add(Op); .)
  597  | (. Op:=nil; .) StatementExpr<Op> (. if Assigned(Op) then OutOp.Children.Add(Op); .)
  598    { (. Op:=nil; .) "," StatementExpr<Op> (. if Assigned(Op) then OutOp.Children.Add(Op); .) }
  599  )
  600  .  
  601  
  602  /*------------------------------------------------------------------------*/
  603  
  604  ForInc<var OutOp : TZcOp>      (. var Op : TZcOp; .)
  605  = (. Op:=nil; 
  606       OutOp := MakeOp(zcBlock);
  607    .) 
  608  (
  609  StatementExpr<Op> (. if Assigned(Op) then OutOp.Children.Add(Op); .) 
  610  { (. Op:=nil; .) "," StatementExpr<Op> (. if Assigned(Op) then OutOp.Children.Add(Op); .) }
  611  )
  612  . 
  613  
  614  
  615  /*------------------------------------------------------------------------*
  616   *----------------------------- Expressions ------------------------------*
  617   *------------------------------------------------------------------------*/
  618  
  619  
  620  Expr<var OutOp : TZcOp>   (. var Op1,Op2 : TZcOp; Kind : TZcAssignType; .)
  621  = (. Op1 :=nil; Op2 := nil; .)
  622    Unary<Op1>
  623    ( OrExpr<Op1,OutOp>
  624        ["?" Expr<Op1> ":" Expr<Op2>
  625           (. if (OutOp=nil) or (Op1=nil) or (Op2=nil) then
  626                SynError(205)
  627              else
  628                OutOp := MakeOp(zcConditional,[OutOp,Op1,Op2]);
  629           .)
  630        ]
  631    | AssignOp<Kind> Expr<Op2>    (. OutOp := MakeAssign(Kind,Op1, Op2); .)
  632    )
  633  .
  634  /*------------------------------------------------------------------------*/
  635  OrExpr<InOp : TZcOp; var OutOp : TZcOp>         (. var Op,L,R : TZcOp; .)
  636  = AndExpr<InOp,OutOp>   (. L := OutOp; .)
  637      {"||" Unary<Op> AndExpr<Op,R>
  638        (. OutOp := MakeOp(zcOr,[L,R]);
  639           L := OutOp; .)
  640      }
  641  .
  642  
  643  
  644  /*------------------------------------------------------------------------*/
  645  AndExpr<InOp : TZcOp; var OutOp : TZcOp>         (. var Op,L,R : TZcOp; .)
  646  = BitOrExpr<InOp,OutOp>  (. L := OutOp; .)
  647      {"&&" Unary<Op> BitOrExpr<Op,R>
  648        (. OutOp := MakeOp(zcAnd,[L,R]);
  649           L := OutOp; .)
  650      }
  651  .
  652  /*------------------------------------------------------------------------*/
  653  BitOrExpr<InOp : TZcOp; var OutOp : TZcOp>         (. var Op,L,R : TZcOp; .)
  654  = BitXorExpr<InOp,OutOp> (. L := OutOp; .)
  655    {"|" Unary<Op> BitXorExpr<Op,R>
  656      (. OutOp := MakeOp(zcBinaryOr,[L,R]);
  657         L := OutOp; .)
  658    }
  659  .
  660  
  661  /*------------------------------------------------------------------------*/
  662  BitXorExpr<InOp : TZcOp; var OutOp : TZcOp>         (. var Op : TZcOp; .)
  663  = BitAndExpr<InOp,OutOp> {"^" (. SynError(205); .) Unary<Op> BitAndExpr<InOp,OutOp>}
  664  .
  665  
  666  /*------------------------------------------------------------------------*/
  667  BitAndExpr<InOp : TZcOp; var OutOp : TZcOp>         (. var Op,L,R : TZcOp; .)
  668  = EqlExpr<InOp,OutOp> (. L := OutOp; .)
  669    {"&" Unary<Op> EqlExpr<Op,R>
  670      (. OutOp := MakeOp(zcBinaryAnd,[L,R]);
  671         L := OutOp; .)
  672    }
  673  .
  674  
  675  /*------------------------------------------------------------------------*/
  676  EqlExpr<InOp : TZcOp; var OutOp : TZcOp>         (. var Op,L,R : TZcOp; Kind : TZcOpKind; .)
  677  = RelExpr<InOp,OutOp>   (. L := OutOp; .)
  678    {( "!="   (. Kind := zcCompNE; .)
  679     | "=="   (. Kind := zcCompEQ; .)
  680     )
  681     Unary<Op> RelExpr<Op,R>
  682           (. OutOp := MakeBinary(Kind, L,R);
  683              L := OutOp;
  684           .)
  685    }
  686  .
  687  /*------------------------------------------------------------------------*/
  688  RelExpr<InOp : TZcOp; var OutOp : TZcOp>         (. var Op,L,R : TZcOp;  Kind : TZcOpKind; .)
  689  = ShiftExpr<InOp,OutOp>         (. L := OutOp; .)
  690    { ( "<"       (. Kind := zcCompLT; .)
  691      | ">"       (. Kind := zcCompGT; .)
  692      | "<="      (. Kind := zcCompLE; .)
  693      | ">="      (. Kind := zcCompGE; .)
  694       )
  695      Unary<Op> ShiftExpr<Op,R>
  696           (. OutOp := MakeBinary(Kind, L,R );
  697              L := OutOp;
  698           .)
  699    }
  700  .
  701  /*------------------------------------------------------------------------*/
  702  ShiftExpr<InOp : TZcOp; var OutOp : TZcOp>         (. var Op,L,R : TZcOp; Kind : TZcOpKind; .)
  703  = AddExpr<InOp,OutOp> (. L := OutOp; .)
  704    { ("<<"  (. Kind := zcBinaryShiftL; .)
  705      | ">>" (. Kind := zcBinaryShiftR; .)
  706      )
  707      Unary<Op> AddExpr<Op,R>
  708      (. OutOp := MakeOp(Kind,[L,R]);
  709         L := OutOp; .)
  710    }
  711  .
  712  /*------------------------------------------------------------------------*/
  713  AddExpr<InOp : TZcOp; var OutOp : TZcOp>         (. var Op,L,R : TZcOp;  Kind : TZcOpKind;.)
  714  = MulExpr<InOp,OutOp>   (. L := OutOp; .)
  715      {( "+"    (. Kind := zcPlus; .)
  716       | "-"    (. Kind := zcMinus; .)
  717       ) Unary<Op> MulExpr<Op,R>
  718           (. 
  719              if (L=nil) or (R=nil) then
  720                ZError('Invalid syntax');
  721              OutOp := MakeBinary(Kind,L,R);
  722              L := OutOp;
  723           .)
  724      }
  725  .
  726  /*------------------------------------------------------------------------*/
  727  MulExpr<InOp : TZcOp; var OutOp : TZcOp>         (. var Tmp : TZcOp; Kind : TZcOpKind; .)
  728  =       (. OutOp := InOp; .)
  729    { ("*"      (. Kind := zcMul; .)
  730      | "/"     (. Kind := zcDiv; .)
  731    /*  | "%" */
  732      ) Unary<Tmp>
  733           (. OutOp := MakeBinary(Kind,InOp,Tmp);
  734              InOp := OutOp;
  735           .)
  736      }
  737  .
  738  /*------------------------------------------------------------------------*/
  739  Unary<var OutOp : TZcOp>         (. var LastOp,Tmp : TZcOp; Kind : TZcOpKind; .)
  740  =       (. LastOp := nil; Kind := zcNop; .)
  741    {
  742      (
  743        /* "+" | */
  744        "-"   (. Kind := zcNegate; .) |
  745        "++"  (. Kind := zcPreInc; .) |    
  746        "--"  (. Kind := zcPreDec; .) |
  747        "!"   (. Kind := zcNot; .)
  748        /* | "~" | "*" */
  749      )
  750      (.
  751         if Kind in [zcNegate,zcNot] then
  752         begin
  753           Tmp := MakeOp(Kind);
  754           if LastOp<>nil then
  755             LastOp.Children.Add(Tmp);
  756           LastOp := Tmp;
  757         end else if Assigned(LastOp) then
  758           ZError('-- and ++ cannot be combined with other unary ops');
  759      .)
  760    }
  761    Primary<Tmp>
  762      (.
  763         if Kind in [zcNop,zcNegate,zcNot] then
  764         begin
  765           if LastOp<>nil then
  766           begin
  767             LastOp.Children.Add(Tmp);
  768             OutOp := LastOp;
  769           end else
  770             OutOp := Tmp;
  771         end else
  772         begin
  773           OutOp := MakePrePostIncDec(Kind,Tmp);
  774         end;
  775      .)
  776  .
  777  /*------------------------------------------------------------------------*/
  778  Primary<var OutOp : TZcOp>
  779  (. var Op : TZcOp; S : string; V : single;
  780       Typ : TZcDataType;
  781  .)
  782  =            (. OutOp := nil; .)
  783    (
  784      ident    (. OutOp := CheckPrimary( MakeIdentifier(LexString) );  .)
  785    | Literal<Typ>  (.
  786          if Typ.Kind in [zctString,zctNull] then
  787            OutOp := TZcOpLiteral.Create(Typ.Kind,LexString)
  788          else
  789          begin
  790            try
  791              S := LexString;
  792              V := ZcStrToFloat(S);
  793              OutOp := TZcOpLiteral.Create(Typ.Kind,V)
  794            except on EConvertError do
  795              SynError(200);
  796            end;
  797          end;
  798       .)
  799    | "(" Expr<OutOp> ")"
  800  /*  | ( "bool"   | "byte"   | "char" | "decimal" | "double" 
  801      | "float"  | "int"    | "long" | "object"  | "sbyte"
  802      | "short"  | "string" | "uint" | "ulong"   | "ushort"
  803      ) "." ident */
  804    )
  805  
  806    {
  807    "++" (. OutOp := MakePrePostIncDec(zcPostInc,OutOp); .)
  808    | "--" (. OutOp := MakePrePostIncDec(zcPostDec,OutOp); .)
  809  
  810  /*  | "->" ident */
  811  
  812    | "." (. IsInIdent := True; .) ident (.
  813  
  814        
  815  
  816        if not (OutOp.Kind in [zcIdentifier,zcSelect,zcFuncCall,zcArrayAccess]) then
  817          ZError('Invalid use of "."');
  818  
  819        Op := MakeOp(zcSelect,LexString);
  820        Op.Children.Add(OutOp);
  821  
  822        OutOp := CheckPrimary(Op);
  823  
  824        IsInIdent := False;
  825  
  826      .)
  827  
  828    | "(" (. Assert(OutOp.Kind=zcIdentifier);
  829             OutOp.Kind:=zcFuncCall;
  830           .)
  831          [Argument<Op> (. OutOp.Children.Add(Op); .)
  832            {"," Argument<Op> (. OutOp.Children.Add(Op); .)
  833            }
  834          ]
  835          (.
  836             if not VerifyFunctionCall(OutOp,S) then
  837               ZError(S);
  838          .)
  839      ")"
  840  
  841    | "[" (. Assert(OutOp.Kind=zcIdentifier);
  842             OutOp.Kind:=zcArrayAccess;
  843           .)
  844      Expr<Op> (. OutOp.Children.Add( MakeCompatible(Op,zctInt) ); .)
  845        {"," Expr<Op> (. OutOp.Children.Add( MakeCompatible(Op,zctInt) ); .) }
  846      "]"
  847  
  848    }
  849  .
  850  /*------------------------------------------------------------------------*/
  851  Literal<var Typ : TZcDataType>
  852  = intCon (. Typ.Kind :=zctInt; .) |
  853    realCon (. Typ.Kind := zctFloat; .) |
  854    stringCon (. Typ.Kind := zctString; .)
  855    | "null" (. Typ.Kind := zctNull; .)
  856    /* | "true" | "false"  */
  857  .
  858  
  859  END Zc.



