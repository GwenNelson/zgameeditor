  566  
  567  
  568  Expr<var OutOp : TZcOp>   (. var Op1,Op2 : TZcOp; Kind : TZcAssignType; .)
  569  = (. Op1 :=nil; Op2 := nil; .)
  570    Unary<Op1>
  571    ( OrExpr<Op1,OutOp>
  572        ["?" Expr<Op1> ":" Expr<Op2>
  573           (. if (OutOp=nil) or (Op1=nil) or (Op2=nil) then
  574                SynError(205)
  575              else
  576                OutOp := MakeOp(zcConditional,[OutOp,Op1,Op2]);
  577           .)
  578        ]
  579    | AssignOp<Kind> Expr<Op2>    (. OutOp := MakeAssign(Kind,Op1, Op2); .)
  580    )
  581  .
  582  /*------------------------------------------------------------------------*/
  583  OrExpr<InOp : TZcOp; var OutOp : TZcOp>         (. var Op,L,R : TZcOp; .)
  584  = AndExpr<InOp,OutOp>   (. L := OutOp; .)
  585      {"||" Unary<Op> AndExpr<Op,R>
  586        (. OutOp := MakeOp(zcOr,[L,R]);
  587           L := OutOp; .)
  588      }
  589  .
  590  
  591  
  592  /*------------------------------------------------------------------------*/
  593  AndExpr<InOp : TZcOp; var OutOp : TZcOp>         (. var Op,L,R : TZcOp; .)
  594  = BitOrExpr<InOp,OutOp>  (. L := OutOp; .)
  595      {"&&" Unary<Op> BitOrExpr<Op,R>
  596        (. OutOp := MakeOp(zcAnd,[L,R]);
  597           L := OutOp; .)
  598      }
  599  .
  600  /*------------------------------------------------------------------------*/
  601  BitOrExpr<InOp : TZcOp; var OutOp : TZcOp>         (. var Op,L,R : TZcOp; .)
  602  = BitXorExpr<InOp,OutOp> (. L := OutOp; .)
  603    {"|" Unary<Op> BitXorExpr<Op,R>
  604      (. OutOp := MakeOp(zcBinaryOr,[L,R]);
  605         L := OutOp; .)
  606    }
  607  .
  608  
  609  /*------------------------------------------------------------------------*/
  610  BitXorExpr<InOp : TZcOp; var OutOp : TZcOp>         (. var Op : TZcOp; .)
  611  = BitAndExpr<InOp,OutOp> {"^" (. SynError(205); .) Unary<Op> BitAndExpr<InOp,OutOp>}
  612  .
  613  
  614  /*------------------------------------------------------------------------*/
  615  BitAndExpr<InOp : TZcOp; var OutOp : TZcOp>         (. var Op,L,R : TZcOp; .)
  616  = EqlExpr<InOp,OutOp> (. L := OutOp; .)
  617    {"&" Unary<Op> EqlExpr<Op,R>
  618      (. OutOp := MakeOp(zcBinaryAnd,[L,R]);
  619         L := OutOp; .)
  620    }
  621  .
  622  
  623  /*------------------------------------------------------------------------*/
  624  EqlExpr<InOp : TZcOp; var OutOp : TZcOp>         (. var Op,L,R : TZcOp; Kind : TZcOpKind; .)
  625  = RelExpr<InOp,OutOp>   (. L := OutOp; .)
  626    {( "!="   (. Kind := zcCompNE; .)
  627     | "=="   (. Kind := zcCompEQ; .)
  628     )
  629     Unary<Op> RelExpr<Op,R>
  630           (. OutOp := MakeBinary(Kind, L,R);
  631              L := OutOp;
  632           .)
  633    }
  634  .
  635  /*------------------------------------------------------------------------*/
  636  RelExpr<InOp : TZcOp; var OutOp : TZcOp>         (. var Op,L,R : TZcOp;  Kind : TZcOpKind; .)
  637  = ShiftExpr<InOp,OutOp>         (. L := OutOp; .)
  638    { ( "<"       (. Kind := zcCompLT; .)
  639      | ">"       (. Kind := zcCompGT; .)
  640      | "<="      (. Kind := zcCompLE; .)
  641      | ">="      (. Kind := zcCompGE; .)
  642       )
  643      Unary<Op> ShiftExpr<Op,R>
  644           (. OutOp := MakeBinary(Kind, L,R );
  645              L := OutOp;
  646           .)
  647    }
  648  .
  649  /*------------------------------------------------------------------------*/
  650  ShiftExpr<InOp : TZcOp; var OutOp : TZcOp>         (. var Op,L,R : TZcOp; Kind : TZcOpKind; .)
  651  = AddExpr<InOp,OutOp> (. L := OutOp; .)
  652    { ("<<"  (. Kind := zcBinaryShiftL; .)
  653      | ">>" (. Kind := zcBinaryShiftR; .)
  654      )
  655      Unary<Op> AddExpr<Op,R>
  656      (. OutOp := MakeOp(Kind,[L,R]);
  657         L := OutOp; .)
  658    }
  659  .
  660  /*------------------------------------------------------------------------*/
  661  AddExpr<InOp : TZcOp; var OutOp : TZcOp>         (. var Op,L,R : TZcOp;  Kind : TZcOpKind;.)
  662  = MulExpr<InOp,OutOp>   (. L := OutOp; .)
  663      {( "+"    (. Kind := zcPlus; .)
  664       | "-"    (. Kind := zcMinus; .)
  665       ) Unary<Op> MulExpr<Op,R>
  666           (. 
  667              if (L=nil) or (R=nil) then
  668                ZError('Invalid syntax');
  669              OutOp := MakeBinary(Kind,L,R);
  670              L := OutOp;
  671           .)
  672      }
  673  .
  674  /*------------------------------------------------------------------------*/
  675  MulExpr<InOp : TZcOp; var OutOp : TZcOp>         (. var Tmp : TZcOp; Kind : TZcOpKind; .)
  676  =       (. OutOp := InOp; .)
  677    { ("*"      (. Kind := zcMul; .)
  678      | "/"     (. Kind := zcDiv; .)
  679    /*  | "%" */
  680      ) Unary<Tmp>
  681           (. OutOp := MakeBinary(Kind,InOp,Tmp);
  682              InOp := OutOp;
  683           .)
  684      }
  685  .
  686  /*------------------------------------------------------------------------*/
  687  Unary<var OutOp : TZcOp>         (. var LastOp,Tmp : TZcOp; Kind : TZcOpKind; .)
  688  =       (. LastOp := nil; Kind := zcNop; .)
  689    {
  690      (
  691        /* "+" | */
  692        "-"   (. Kind := zcNegate; .) |
  693        "++"  (. Kind := zcPreInc; .) |    
  694        "--"  (. Kind := zcPreDec; .) |
  695        "!"   (. Kind := zcNot; .)
  696        /* | "~" | "*" */
  697      )
  698      (.
  699         if Kind in [zcNegate,zcNot] then
  700         begin
  701           Tmp := MakeOp(Kind);
  702           if LastOp<>nil then
  703             LastOp.Children.Add(Tmp);
  704           LastOp := Tmp;
  705         end else if Assigned(LastOp) then
  706           ZError('-- and ++ cannot be combined with other unary ops');
  707      .)
  708    }
  709    Primary<Tmp>
  710      (.
  711         if Kind in [zcNop,zcNegate,zcNot] then
  712         begin
  713           if LastOp<>nil then
  714           begin
  715             LastOp.Children.Add(Tmp);
  716             OutOp := LastOp;
  717           end else
  718             OutOp := Tmp;
  719         end else
  720         begin
  721           OutOp := MakePrePostIncDec(Kind,Tmp);
  722         end;
  723      .)
  724  .
  725  /*------------------------------------------------------------------------*/
  726  Primary<var OutOp : TZcOp>
  727  (. var Op : TZcOp; S : string; V : single;
  728       Typ : TZcDataType;
  729  .)
  730  =            (. OutOp := nil; .)
  731    (
  732      ident    (. OutOp := CheckPrimary( MakeIdentifier(LexString) );  .)
  733    | Literal<Typ>  (.
  734          if Typ.Kind in [zctString,zctNull] then
  735            OutOp := TZcOpLiteral.Create(Typ.Kind,LexString)
  736          else
  737          begin
  738            try
  739              S := LexString;
  740              V := ZcStrToFloat(S);
  741              OutOp := TZcOpLiteral.Create(Typ.Kind,V)
  742            except on EConvertError do
  743              SynError(200);
  744            end;
  745          end;
  746       .)
  747    | "(" Expr<OutOp> ")"
  748  /*  | ( "bool"   | "byte"   | "char" | "decimal" | "double" 
  749      | "float"  | "int"    | "long" | "object"  | "sbyte"
  750      | "short"  | "string" | "uint" | "ulong"   | "ushort"
  751      ) "." ident */
  752    )
  753  
  754    {
  755    "++" (. OutOp := MakePrePostIncDec(zcPostInc,OutOp); .)
  756    | "--" (. OutOp := MakePrePostIncDec(zcPostDec,OutOp); .)
  757  
  758  /*  | "->" ident */
  759  
  760    | "." (. IsInIdent := True; .) ident (.
  761  
  762        
  763  
  764        if not (OutOp.Kind in [zcIdentifier,zcSelect,zcFuncCall,zcArrayAccess]) then
  765          ZError('Invalid use of "."');
  766  
  767        Op := MakeOp(zcSelect,LexString);
  768        Op.Children.Add(OutOp);
  769  
  770        OutOp := CheckPrimary(Op);
  771  
  772        IsInIdent := False;
  773  
  774      .)
  775  
  776    | "(" (. Assert(OutOp.Kind=zcIdentifier);
  777             OutOp.Kind:=zcFuncCall;
  778           .)
  779          [Argument<Op> (. OutOp.Children.Add(Op); .)
  780            {"," Argument<Op> (. OutOp.Children.Add(Op); .)
  781            }
  782          ]
  783          (.
  784             if not VerifyFunctionCall(OutOp,S) then
  785               ZError(S);
  786          .)
  787      ")"
  788  
  789    | "[" (. Assert(OutOp.Kind=zcIdentifier);
  790             OutOp.Kind:=zcArrayAccess;
  791           .)
  792      Expr<Op> (. OutOp.Children.Add( MakeCompatible(Op,zctInt) ); .)
  793        {"," Expr<Op> (. OutOp.Children.Add( MakeCompatible(Op,zctInt) ); .) }
  794      "]"
  795  
  796    }
  797  .
  798  /*------------------------------------------------------------------------*/
  799  Literal<var Typ : TZcDataType>
  800  = intCon (. Typ.Kind :=zctInt; .) |
  801    realCon (. Typ.Kind := zctFloat; .) |
  802    stringCon (. Typ.Kind := zctString; .)
  803    | "null" (. Typ.Kind := zctNull; .)
  804    /* | "true" | "false"  */
  805  .
  806  
  807  END Zc.



