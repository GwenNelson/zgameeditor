  421           Cns._Type := dvbFloat;
  422           Cns.Value := Lit.Value;
  423         end;
  424       zctString:
  425         begin
  426           Cns._Type := dvbString;
  427           Cns.SetString('StringValue',AnsiString(Lit.StringValue));
  428         end;
  429       else
