  485  
  486  Block<var OutOp : TZcOp>
  487    (. var
  488         Op : TZcOp;
  489    .)
  490  = (. Op := nil;
  491       OutOp := MakeOp(zcBlock);
  492    .)
  493    "{" {Statement<Op> (. if Assigned(Op) then OutOp.Children.Add(Op); .) } "}".
  494  
  495  /*------------------------------------------------------------------------*/
  496  StatementExpr<var OutOp : TZcOp>           (. var Op1,Op2 : TZcOp; Kind : TZcAssignType; .)
  497  = (. Op1 :=nil; Op2 := nil; .)
  498    Unary<Op1>
  499    ( AssignOp<Kind> Expr<Op2>
  500      (.
  501        if Op2<>nil then
  502          OutOp := MakeAssign(Kind,Op1,Op2);
  503      .)
  504    |                             (. OutOp := Op1; .)
  505    )
  506  .
  507  
  508  /*------------------------------------------------------------------------*/
  509  AssignOp<var Kind : TZcAssignType>
  510  = "="     (. Kind := atAssign; .)
  511    | "+="  (. Kind := atPlusAssign; .)
  512    | "-="  (. Kind := atMinusAssign; .)
  513    | "*="  (. Kind := atMulAssign; .)
  514    | "/="  (. Kind := atDivAssign; .)
  515  /*  | "+=" | "-=" | "*=" | "/=" | "%=" | "&=" | "|=" | "^=" | "<<=" | ">>=" */
  516  .
