  548  StatementExpr<var OutOp : TZcOp>           (. var Op1,Op2 : TZcOp; Kind : TZcAssignType; .)
  549  = (. Op1 :=nil; Op2 := nil; .)
  550    Unary<Op1>
  551    ( AssignOp<Kind> Expr<Op2>
  552      (.
  553        if Op2<>nil then
  554          OutOp := MakeAssign(Kind,Op1,Op2);
  555      .)
  556    |                             (. OutOp := Op1; .)
