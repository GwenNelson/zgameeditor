  518  /*------------------------------------------------------------------------*/
  519  
  520  SwitchSection<SwitchOp : TZcOpSwitch> (. var Op : TZcOp; .)
  521  = (. Op := nil; .)
  522    SwitchLabel<Op>   (. AddSwitchLabel(SwitchOp,Op); .)
  523    {
  524      SwitchLabel<Op> (. AddSwitchLabel(SwitchOp,Op); .)
  525    }
  526    Statement<Op>    (. if Assigned(Op) then SwitchOp.StatementsOps.Last.Children.Add(Op); .)
  527    {
  528       Statement<Op> (. if Assigned(Op) then SwitchOp.StatementsOps.Last.Children.Add(Op); .)
  529    }
  530  .
  531  
  532  /*------------------------------------------------------------------------*/
  533  
  534  SwitchLabel<var OutOp : TZcOp>
  535  = "case" Expr<OutOp> ":"
  536  | "default" ":"
  537  .
